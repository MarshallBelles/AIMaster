{
  "name": "aimaster",
  "version": "1.0.0",
  "description": "AIMaster (AIM) - Simple AI agent with tool support for local LLMs",
  "type": "module",
  "main": "agent.mjs",
  "scripts": {
    "start": "node agent.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf output/tests/* output/temp/* output/logs/* output/artifacts/*",
    "clean:all": "rm -rf output/*",
    "clean:reports": "rm -rf output/reports/*",
    "setup:output": "mkdir -p output/{tests,logs,reports,temp,artifacts}"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "tools",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "inquirer": "^12.6.3",
    "nunjucks": "^3.2.4",
    "ora": "^8.2.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.1",
    "vitest": "^3.2.1"
  }
}